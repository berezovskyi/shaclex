prefix : <http://example.org/>
prefix foaf: <http://xmlns.com/foaf/0.1/>

start = @:S

:S { ( foaf:name .
     | foaf:givenName .; foaf:familyName .
     ); # either a name or components
     foaf:shoeSize .;
    ( foaf:homepage . ; ! foaf:name . )? # no foaf:homepage if there's a foaf:name.
}

:NotS NOT @:S 
